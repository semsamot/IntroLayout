<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IntroLayout">
        <attr name="overlay_color" format="color" />
        <attr name="target_highlight_color" format="color" />
        <attr name="target_shape_border_color" format="color" />
        <attr name="target_highlight_shape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="hexagon" value="2" />
        </attr>
        <attr name="arrow_color" format="color" />
        <attr name="arrow_stroke_width" format="integer" />
    </declare-styleable>
</resources>